int main()
{
    int n;
    printf("enter size of array");
    scanf("%d",&n);
    int a[n],m,pos,value;
    printf("Enter no. of elements");
    scanf("%d",&m);
    if(m>=n)
    printf("Insertion is not possible");
    else{
        for( int i=0;i<m;i++){
         scanf("%d/n",&a[i]);   
        }
        printf("Enter position and value to be inserted");
        scanf("%d%d",&pos,&value);
        for( int i=m-1;i>=pos;i--)
        {
            a[i+1]=a[i];
        }
        a[pos]=value;
        m++;
        for( int i=0;i<m;i++)
        {
            printf("%d,",a[i]);
        }
        }

Â Â Â Â returnÂ 0;
}

