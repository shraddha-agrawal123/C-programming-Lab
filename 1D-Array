#include <stdio.h>

int main()
{
    int i,n,arr[50],count=0;
    printf("enter no. of elements want to insert");
    scanf("%d",&n);
    for(i=0;i<=n;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<=n;i++)
    {
        if(arr[i]<0){
            count++;
        }
    }
    printf("total negative no. in array is %d",count);

    return 0;
}

