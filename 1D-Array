#include <stdio.h>

int main()
{
    int i,n,arr[50];
    printf("enter no of elements in array");
    scanf("%d",&n);
    printf("enter array");
    for(i=0;i<=n;i++){
        scanf("%d",&arr[i]);
    }
    int max=arr[0];
    for(i=0;i<=n;i++){
        if(max<arr[i]){
            max=arr[i];
        }
    }
    printf("Maximum is %d\n",max);
     int min=arr[0];
    for(i=0;i<=n;i++){
        if(arr[i]<min){
            min=arr[i];
        }
    }
    printf("Minimum is %d",min);

    return 0;
}

