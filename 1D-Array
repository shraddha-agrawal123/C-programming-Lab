    printf("Enter the elements");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    for(int i=1;i<n;i++)
    {
        if(a[i]>max)
        max=a[i];
    }
    max++;
    int b[100]={0};
    for(int i=0;i<n;i++)
    {
        b[a[i]]++;
    }
    for(int i=0;i<max;i++)
    {
        if(b[i]>0)
        printf(" The frequency of %d=%d\n",i,b[i]);
    }
    
