#include <stdio.h>

int main()
{
    int n ,max=0;
    printf("Enter no. of elements");
    scanf("%d",&n);
    int a[n];
    printf("Enter the elements");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    for(int i=1;i<n;i++)
    {
        if(a[i]>max)
        max=a[i];
    }
    max++;
    int b[100]={0};
    for(int i=0;i<n;i++)
    {
        b[a[i]]++;
    }
    for(int i=0;i<max;i++)
    {
        if(b[i]>1)
        printf(" The Duplicate elementis %d\n",i);
    }
    
    return 0;
}

ğ‚ ğ©ğ«ğ¨ğ ğ«ğšğ¦ ğ­ğ¨ ğœğ¨ğ®ğ§ğ­ ğ­ğ¨ğ­ğšğ¥ ğ§ğ®ğ¦ğ›ğğ« ğ¨ğŸ ğğ®ğ©ğ¥ğ¢ğœğšğ­ğ ğğ¥ğğ¦ğğ§ğ­ğ¬ ğ¢ğ§ ğšğ§ ğšğ«ğ«ğšğ².
#include <stdio.h>

int main()
{
    int n ,i,j,max,count=0;
    printf("Enter no. of elements");
    scanf("%d",&n);
    int a[n];
    printf("Enter the elements");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        for(j=i+1;j<n;j++){
            if(a[i] == a[j])
            {
                count++;
                break;
            }
        }
    }
        printf("\nTotal number of duplicate elements found in array = %d", count);
    
    return 0;
}

