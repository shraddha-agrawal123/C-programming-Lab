#include <stdio.h>
void array(int n,int*a){
     int max=a[0];
     for(int i=0;i<n;i++) {
       if(max<a[i]){
           max=a[i];
       }  
     }
    printf(" Maximum is %d ",max);
}
int main(){
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    array(n,a);
    return 0;
}
