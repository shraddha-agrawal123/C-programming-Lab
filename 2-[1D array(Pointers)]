To print all the negative elements in array

#include <stdio.h>
void array(int n,int*a){
    for(int i=0;i<n;i++)
    {
        if(a[i]<0){
           printf("%d ",a[i]);
        }
     
    }
}
int main(){
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    array(n,a);
}
