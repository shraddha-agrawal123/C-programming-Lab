#include <string.h>

int main()
{
 char a[100], c;
  int i,len,count=0;
printf("Enter string");
gets(a);
printf("\nEnter the character which u find the first occurence\n");
c=getchar();
for(i=0;a[i];i++){
    if(a[i]==c)
    {
		 count++;
 
		}
}
printf("total occurence of %c is %d",c,count);
    return 0;
}
