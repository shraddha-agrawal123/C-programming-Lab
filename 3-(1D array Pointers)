C program to find sum of all array elements.
#include <stdio.h>
void array(int n,int*a){
     int sum=0;
    for(int i=0;i<n;i++)
    {
        sum=sum+a[i];
        
     
    }
    printf("%d ",sum);
}
int main(){
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    array(n,a);
}
