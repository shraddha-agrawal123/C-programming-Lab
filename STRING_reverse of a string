#include <stdio.h>
#include<string.h>
#include<ctype.h>
int main()
{
    char a[100],b[100];
    int i,j;
    printf("Enter string\n");
    scanf("%[^\n]c",a);
    int len=strlen(a);
    j=len-1;
    for(i=0;i<len;i++){
        b[i]=a[j];
        j--;
    }
    b[i]='\0';
    printf("%s\n",b);
    
    return 0;
}
